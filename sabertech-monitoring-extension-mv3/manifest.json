{
    "manifest_version": 3,
    "name": "Sabertechs Monitor",
    "version": "1.1",
    "content_scripts": [
        {
          "matches": ["https://*/*"],
          "css": [],
          "js": ["content.js"]
        }
      ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "idle",
        "chrome://favicon/*",
        "webNavigation",
        "unlimitedStorage"
    ],
}